$("#page-result-info").empty();
$("#pagination-results").remove(); 
<% js = escape_javascript(render(partial: 'subjects/list', locals: { subjects: @subjects })) %>
$("#_results").html("<%= js %>");
$("#page-result-info").html("<%= escape_javascript(page_entries_info @subjects, model: 'Subject') %>");
$('#show-target').empty();
$('#show-target').append('<%= j render(partial: 'subjects/show', locals: {subject: @subjects.first}) %>');
$(window.location.replace("<%= subjects_path(search: params[:search]) %>"));